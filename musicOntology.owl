@prefix : <http://www.semanticweb.org/musicOntology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/musicOntology> .

<http://www.semanticweb.org/musicOntology> rdf:type owl:Ontology .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.semanticweb.org/musicOntology#collectiveRole

:collectiveRole rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                
                rdfs:domain :CollectiveMember ;
                
                rdfs:range :CollectiveRole .



###  http://www.semanticweb.org/musicOntology#composer

:composer rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          
          rdfs:domain :MusicalComposition ;
          
          rdfs:range :Musician ,
                     :Performer ;
          
          owl:inverseOf :composer_of .



###  http://www.semanticweb.org/musicOntology#composer_of

:composer_of rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             
             rdfs:range :MusicalComposition ;
             
             rdfs:domain :Musician ,
                         :Performer .



###  http://www.semanticweb.org/musicOntology#from_album

:from_album rdf:type owl:ObjectProperty ;
            
            rdfs:range :Album ;
            
            rdfs:domain :Song ;
            
            owl:inverseOf :has_song .



###  http://www.semanticweb.org/musicOntology#has_album

:has_album rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           
           rdfs:range :Album ;
           
           rdfs:domain :Performer .



###  http://www.semanticweb.org/musicOntology#has_genre

:has_genre rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           
           rdfs:range :Genre ;
           
           rdfs:domain :Performer .



###  http://www.semanticweb.org/musicOntology#has_member

:has_member rdf:type owl:ObjectProperty ;
            
            rdfs:domain :Collective ;
            
            rdfs:range :CollectiveMember .



###  http://www.semanticweb.org/musicOntology#has_song

:has_song rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          
          rdfs:domain :Album ;
          
          rdfs:range :Song .



###  http://www.semanticweb.org/musicOntology#musician

:musician rdf:type owl:ObjectProperty ;
          
          rdfs:domain :CollectiveMember ;
          
          rdfs:range :Musician ;
          
          rdfs:domain :SoloPerformer .



###  http://www.semanticweb.org/musicOntology#performer

:performer rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           
           rdfs:domain :Album ,
                       :MusicalComposition ;
           
           rdfs:range :Performer .



###  http://www.semanticweb.org/musicOntology#performs

:performs rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          
          rdfs:range :MusicalComposition ;
          
          rdfs:domain :Performer ;
          
          owl:inverseOf :performer .



###  http://www.semanticweb.org/musicOntology#plays_on

:plays_on rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          
          rdfs:range :MusicalInstrument ;
          
          rdfs:domain :Musician .



###  http://www.semanticweb.org/musicOntology#primaryMusicalInstrument

:primaryMusicalInstrument rdf:type owl:ObjectProperty ,
                                   owl:TransitiveProperty ;
                          
                          rdfs:range :MusicalInstrument ;
                          
                          rdfs:domain :Musician .



###  http://www.semanticweb.org/musicOntology#recorded_on

:recorded_on rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             
             rdfs:domain :Album ;
             
             rdfs:range :Medium ;
             
             rdfs:domain :MusicalComposition .



###  http://www.w3.org/2002/07/owl#topObjectProperty

owl:topObjectProperty rdf:type owl:TransitiveProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://www.semanticweb.org/musicOntology#name

:name rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      
      rdfs:domain :Album ,
                  :Medium ,
                  :Musician ,
                  :Performer ;
      
      rdfs:range xsd:string .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.semanticweb.org/musicOntology#Aerophone

:Aerophone rdf:type owl:Class ;
           
           rdfs:label "Aerophone" ;
           
           rdfs:subClassOf :MusicalInstrument ;
           
           rdfs:comment "musical instruments, which are wood, metal and other tubes with various devices and forms, emits a musical sounds as a result of fluctuations in them prisoner air column." .



###  http://www.semanticweb.org/musicOntology#African

:African rdf:type owl:Class ;
         
         rdfs:label "African" ;
         
         rdfs:subClassOf :Genre ;
         
         rdfs:comment "African popular music, like African traditional music, is vast and varied. Most contemporary genres of African popular music build on cross-pollination with western popular music." .



###  http://www.semanticweb.org/musicOntology#Album

:Album rdf:type owl:Class ;
       
       rdfs:label "Album" ;
       
       rdfs:comment "An album is a recording originally issued in gramophone format, followed by the vinyl format, and has then been issued in other formats such as Compact Cassette, Compact disc, and increasingly in digital formats such as MP3, AAC, ALAC, or FLAC. Audio albums in physical form are often provided with decorative covers (cover art) and liner notes and inserts about the music and recording, giving background information and analysis of the recording, lyrics and librettos, images of the performers, and other images and text, as well as thanking contributors. When supplied with compact discs they are known as CD booklets." .



###  http://www.semanticweb.org/musicOntology#Asian

:Asian rdf:type owl:Class ;
       
       rdfs:label "Asian" ;
       
       rdfs:subClassOf :Genre ;
       
       rdfs:comment "Asian music encompasses numerous different musical styles originating from a large number of Asian countries." .



###  http://www.semanticweb.org/musicOntology#Avant-garde

:Avant-garde rdf:type owl:Class ;
             
             rdfs:label "Avant-garde" ;
             
             rdfs:subClassOf :Genre ;
             
             rdfs:comment "The term avant-garde is applied to music when it is considered \"ahead of its time\" by critics or fans, i.e., it contains unique or original elements, or it explores unusual fusions of different genres." .



###  http://www.semanticweb.org/musicOntology#BallroomDance

:BallroomDance rdf:type owl:Class ;
               
               rdfs:label "Ballroom dance" ;
               
               rdfs:subClassOf :Genre ;
               
               rdfs:comment "Ballroom dance is a set of partner dances, which are enjoyed both socially and competitively around the world, and growing all the more popular in North America. Because of its performance and entertainment aspects, ballroom dance is also widely enjoyed on stage, film, and television." .



###  http://www.semanticweb.org/musicOntology#Blues

:Blues rdf:type owl:Class ;
       
       rdfs:label "Blues" ;
       
       rdfs:subClassOf :Genre ;
       
       rdfs:comment "Blues is a genre and musical form that originated in African-American communities in the \"Deep South\" of the United States around the end of the 19th century. The genre is a fusion of traditional African music and European folk music, spirituals, work songs, field hollers, shouts and chants, and rhymed simple narrative ballads. The blues form, ubiquitous in jazz, rhythm and blues and rock and roll, is characterized by the call-and-response pattern and specific chord progressions, of which the twelve-bar blues is the most common. The blue notes are also an important part of the sound. Blues shuffles or walking bass reinforce the trance-like rhythm and form a repetitive effect called a groove" .



###  http://www.semanticweb.org/musicOntology#Brazilian

:Brazilian rdf:type owl:Class ;
           
           rdfs:label "Brazilian" ;
           
           rdfs:subClassOf :Latin ;
           
           rdfs:comment "The music of Brazil encompasses various regional music styles influenced by African, European and Amerindian forms. After 500 years of history, Brazilian music developed some unique and original styles such as samba, bossa nova, MPB, sertanejo, pagode, tropicalia, choro, maracatu, embolada (coco de repente), Mangue Bit, funk carioca (in Brazil simply known as Funk), frevo, forró, axé, brega, lambada, and Brazilian versions of foreign musical genres, such as Brazilian rock and rap." .



###  http://www.semanticweb.org/musicOntology#Caribbean-and-Caribbean-influenced

:Caribbean-and-Caribbean-influenced rdf:type owl:Class ;
                                    
                                    rdfs:label "Caribbean and Caribbean-influenced" ;
                                    
                                    rdfs:subClassOf :Genre ;
                                    
                                    rdfs:comment "Caribbean music genres are diverse. They are each syntheses of African, European, Indian and Indigenous influences, largely created by descendants of African slaves (see Afro-Caribbean music), along with contributions from other communities (such as Indo-Caribbean music). Some of the styles to gain wide popularity outside of the Caribbean include bouyon, cadence-lypso, calypso, compas, Jing ping, punta, reggae, reggaeton, soca, and zouk. Caribbean is also related to Central American and South American music." .



###  http://www.semanticweb.org/musicOntology#Chordophone

:Chordophone rdf:type owl:Class ;
             
             rdfs:label "Chordophone" ;
             
             rdfs:subClassOf :MusicalInstrument ;
             
             rdfs:comment "is a musical instrument in which the sound source (vibrator) are the vibration of the strings." .



###  http://www.semanticweb.org/musicOntology#Collective

:Collective rdf:type owl:Class ;
            
            rdfs:label "Collective" ;
            
            rdfs:subClassOf [ rdf:type owl:Class ;
                              owl:intersectionOf ( :Performer
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :has_member ;
                                                     owl:onClass :CollectiveMember ;
                                                     owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger
                                                   ]
                                                 )
                            ] ;
            
            owl:disjointWith :SoloPerformer .



###  http://www.semanticweb.org/musicOntology#CollectiveMember

:CollectiveMember rdf:type owl:Class ;
                  
                  rdfs:label "Collective member" ;
                  
                  rdfs:subClassOf [ rdf:type owl:Class ;
                                    owl:intersectionOf ( :Performer
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :collectiveRole ;
                                                           owl:onClass :CollectiveRole ;
                                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :musician ;
                                                           owl:onClass :Musician ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                         ]
                                                       )
                                  ] .



###  http://www.semanticweb.org/musicOntology#CollectiveRole

:CollectiveRole rdf:type owl:Class ;
                
                rdfs:label "Collective role" .



###  http://www.semanticweb.org/musicOntology#Comedy

:Comedy rdf:type owl:Class ;
        
        rdfs:label "Comedy" ;
        
        rdfs:subClassOf :Genre ;
        
        rdfs:comment "Comedy music is music that is comic or humorous in nature, encompassing a wide variety of music genres. Popular types of comedy music include parody music, novelty songs, comedy rock and comedy hip hop." .



###  http://www.semanticweb.org/musicOntology#Country

:Country rdf:type owl:Class ;
         
         rdfs:label "Country" ;
         
         rdfs:subClassOf :Genre ;
         
         rdfs:comment "Country music is a genre of American popular music that originated in Southern United States, in Atlanta, Georgia in the 1920s. It takes its roots from the southeastern genre of American folk music and Western music. Blues modes have been used extensively throughout its recorded history. Country music often consists of ballads and dance tunes with generally simple forms and harmonies accompanied by mostly string instruments such as banjos, electric and acoustic guitars, fiddles, and harmonicas." .



###  http://www.semanticweb.org/musicOntology#Duet

:Duet rdf:type owl:Class ;
      
      rdfs:label "Duet" ;
      
      rdfs:subClassOf [ rdf:type owl:Class ;
                        owl:intersectionOf ( :Collective
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :has_member ;
                                               owl:onClass :CollectiveMember ;
                                               owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger
                                             ]
                                           )
                      ] ;
      
      rdfs:comment "A duet is a musical composition for two performers in which the performers have equal importance to the piece. It is often used to describe a composition involving two singers. It differs from a harmony, as the performers take turns performing a solo section rather than performing simultaneously. In classical music, the term is most often used for a composition for two singers or pianists. A piece performed by two pianists performing together on the same piano is referred to as \"piano duet\" or \"piano four hands\". A piece for two pianists performing together on separate pianos is referred to as a \"piano duo\"." .



###  http://www.semanticweb.org/musicOntology#EastAsian

:EastAsian rdf:type owl:Class ;
           
           rdfs:label "East Asian" ;
           
           rdfs:subClassOf :Asian ;
           
           owl:disjointWith :South-and-southeastAsian .



###  http://www.semanticweb.org/musicOntology#Easy-listening

:Easy-listening rdf:type owl:Class ;
                
                rdfs:label "Easy listening" ;
                
                rdfs:subClassOf :Genre ;
                
                rdfs:comment "Easy listening (also known as orchestral pop) is a popular music genre and radio format that was most popular during the 1950s to 1970s. It is related to middle-of-the-road (MOR) music and encompasses instrumental recordings of standards, hit songs and popular non-rock vocals. It was differentiated from the mostly instrumental beautiful music format by its variety of styles, including a percentage of vocals, arrangements and tempos to fit various day parts during the broadcast day." .



###  http://www.semanticweb.org/musicOntology#Electronic

:Electronic rdf:type owl:Class ;
            
            rdfs:label "Electronic" ;
            
            rdfs:subClassOf :Genre ;
            
            rdfs:comment "Electronic music is music that employs electronic musical instruments and electronic music technology in its production, an electronic musician being a musician who composes and/or performs such music. In general a distinction can be made between sound produced using electromechanical means and that produced using electronic technology. Examples of electromechanical sound producing devices include the telharmonium, Hammond organ, and the electric guitar. Purely electronic sound production can be achieved using devices such as the theremin, sound synthesizer, and computer." .



###  http://www.semanticweb.org/musicOntology#Electrophone

:Electrophone rdf:type owl:Class ;
              
              rdfs:label "Electrophone" ;
              
              rdfs:subClassOf :MusicalInstrument ;
              
              rdfs:comment "musical instruments, are used to generate (including fusion) sound different electronic circuits. Such tools can create sound by varying the volume, frequency, or duration of each segment generated." .



###  http://www.semanticweb.org/musicOntology#Ensemble

:Ensemble rdf:type owl:Class ;
          
          rdfs:label "Ensemble" ;
          
          rdfs:subClassOf [ rdf:type owl:Class ;
                            owl:intersectionOf ( :Collective
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :has_member ;
                                                   owl:onClass :CollectiveMember ;
                                                   owl:minQualifiedCardinality "9"^^xsd:nonNegativeInteger
                                                 ]
                                               )
                          ] ;
          
          rdfs:comment "A musical ensemble, also known as a music group, is a group of people who perform instrumental or vocal music, typically known by a distinct name. In classical music, trios or quartets either blend the sounds of musical instrument families (such as piano, strings, and wind instruments) or group together instruments from the same instrument family, such as string ensembles or wind ensembles. In jazz ensembles, the instruments typically include wind instruments (one or more saxophones, trumpets, etc.), one or two chordal \"comping\" instruments (electric guitar, piano, or organ), a bass instrument (bass guitar or double bass), and a drummer or percussionist. In rock ensembles, usually called rock bands, there are usually guitars and keyboards (piano, electric piano, Hammond organ, synthesizer, etc.) and a rhythm section made up of a bass guitar and drum kit." .



###  http://www.semanticweb.org/musicOntology#Folk

:Folk rdf:type owl:Class ;
      
      rdfs:label "Folk" ;
      
      rdfs:subClassOf :Genre ;
      
      rdfs:comment "Folk music includes both traditional music and the genre that evolved from it during the 20th century folk revival. The term originated in the 19th century but is often applied to music that is older than that. Some types of folk music are also called world music." .



###  http://www.semanticweb.org/musicOntology#Genre

:Genre rdf:type owl:Class ;
       
       rdfs:label "Genre" ;
       
       rdfs:comment "conventional category that identifies pieces of music as belonging to a shared tradition or set of conventions" .



###  http://www.semanticweb.org/musicOntology#Hip-hop

:Hip-hop rdf:type owl:Class ;
         
         rdfs:label "Hip hop" ;
         
         rdfs:subClassOf :Genre ;
         
         rdfs:comment "Hip hop music, also called hip-hop, rap music, or hip-hop music, is a music genre consisting of a stylized rhythmic music that commonly accompanies rapping, a rhythmic and rhyming speech that is chanted. It developed as part of hip hop culture, a subculture defined by four key stylistic elements: MCing/rapping, DJing/scratching, break dancing, and graffiti writing. Other elements include sampling (or synthesis), and beatboxing. While often used to refer to rapping, \"hip hop\" more properly denotes the practice of the entire subculture. The term hip hop music is sometimes used synonymously with the term rap music, though rapping is not a required component of hip hop music; the genre may also incorporate other elements of hip hop culture, including DJing and scratching, beatboxing, and instrumental tracks." .



###  http://www.semanticweb.org/musicOntology#Idiophone

:Idiophone rdf:type owl:Class ;
           
           rdfs:label "Idiophone" ;
           
           rdfs:subClassOf :Percussion ;
           
           owl:disjointWith :Membranophone ;
           
           rdfs:comment "musical instrument, the sound source in which the body itself is a tool, or a portion thereof, is not required for pre-tensioning of sound or compression (stretched string or membrane)." .



###  http://www.semanticweb.org/musicOntology#Jazz

:Jazz rdf:type owl:Class ;
      
      rdfs:label "Jazz" ;
      
      rdfs:subClassOf :Genre ;
      
      rdfs:comment "Jazz is a genre of music that originated in African American communities during the late 19th and early 20th century. Jazz emerged in many parts of the United States of independent popular musical styles; linked by the common bonds of African American and European American musical parentage with a performance orientation. Jazz spans a range of music from ragtime to the present day—a period of over 100 years—and has proved to be very difficult to define. Jazz makes heavy use of improvisation, polyrhythms, syncopation, and the swung note, as well as aspects of European harmony, American popular music, the brass band tradition, and African musical elements such as blue notes and ragtime. A musical group that plays jazz is called a jazz band. The birth of Jazz in the multicultural society of America has lead intellectuals from around the world to refer to Jazz as \"one of America’s original art forms\"" .



###  http://www.semanticweb.org/musicOntology#KeyboardInstrument

:KeyboardInstrument rdf:type owl:Class ;
                    
                    rdfs:label "Keyboard instrument" ;
                    
                    rdfs:subClassOf :MusicalInstrument ;
                    
                    rdfs:comment "musical instruments, extract sound that made using a system of levers and controlled using keys located in a certain order and constituting a keyboard instrument." .



###  http://www.semanticweb.org/musicOntology#Latin

:Latin rdf:type owl:Class ;
       
       rdfs:label "Latin" ;
       
       rdfs:subClassOf :Genre ;
       
       rdfs:comment "Latin American music refers to music originating from Latin America namely Spanish- and Portuguese-speaking countries of the Americas and the Caribbean. Latin American music also incorporates African music from slaves who were to transported to the America by European and settles as well as music from Indigenous peoples of the Americas. The first of those encapsulates all music styles generated from Latin countries, such as salsa, merengue, tango, compas, bossa nova and bachata; as well as other styles derived from a more mainstream genre, such as Latin pop, rock, jazz and reggaeton." .



###  http://www.semanticweb.org/musicOntology#Medium

:Medium rdf:type owl:Class ;
        
        rdfs:label "Medium" ;
        
        rdfs:comment "A means or instrumentality for storing or communicating musical manifestation." .



###  http://www.semanticweb.org/musicOntology#Membranophone

:Membranophone rdf:type owl:Class ;
               
               rdfs:subClassOf :Percussion ;
               
               rdfs:comment """musical instrument in which the element produces a sound is stretched membrane.
Most membranophones drums are (but without there drums membrane made of whole piece of wood belonging to idiophone). In addition to drums, to membranophones include a special class of instruments in which the membrane alters the sound of voices.""" .



###  http://www.semanticweb.org/musicOntology#MusicalComposition

:MusicalComposition rdf:type owl:Class ;
                    
                    rdfs:label "Musical composition" ;
                    
                    rdfs:comment """Musical composition can refer to an original piece of music, the structure of a musical piece, or the process of creating a new piece of music. People who practice composition are called composers.
Although today composition is considered to consist of the manipulation of each aspect of music (harmony, melody, form, rhythm, and timbre), according to Jean-Benjamin de Laborde (1780)""" .



###  http://www.semanticweb.org/musicOntology#MusicalInstrument

:MusicalInstrument rdf:type owl:Class ;
                   
                   rdfs:label "Musical instrument" ;
                   
                   rdfs:comment "A musical instrument is an instrument created or adapted to make musical sounds. In principle, any object that produces sound can be a musical instrument—it is through purpose that the object becomes a musical instrument." .



###  http://www.semanticweb.org/musicOntology#Musician

:Musician rdf:type owl:Class ;
          
          rdfs:label "Musician" ;
          
          rdfs:comment "A musician (or instrumentalist) is a person who plays a musical instrument or is musically talented, or one who composes, conducts, or performs music." .



###  http://www.semanticweb.org/musicOntology#Nocturne

:Nocturne rdf:type owl:Class ;
          
          rdfs:label "Nocturne" ;
          
          rdfs:subClassOf :MusicalComposition ;
          
          rdfs:comment "A nocturne (from the French which meant nocturnal, from Latin nocturnus) is usually a musical composition that is inspired by, or evocative of, the night. Historically, nocturne is a very old term applied to night Offices and, since the Middle Ages, to divisions in the canonical hour of Matins." .



###  http://www.semanticweb.org/musicOntology#Occasional

:Occasional rdf:type owl:Class ;
            
            rdfs:label "Occasional" ;
            
            rdfs:subClassOf :OtherGenres ;
            
            rdfs:comment "Occasional music: Military music, marches, national anthems and related compositions" .



###  http://www.semanticweb.org/musicOntology#Octet

:Octet rdf:type owl:Class ;
       
       rdfs:label "Octet" ;
       
       rdfs:subClassOf [ rdf:type owl:Class ;
                         owl:intersectionOf ( :Collective
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :has_member ;
                                                owl:onClass :CollectiveMember ;
                                                owl:qualifiedCardinality "8"^^xsd:nonNegativeInteger
                                              ]
                                            )
                       ] ;
       
       rdfs:comment "In music, an octet is a musical ensemble consisting of eight instruments or voices, or a musical composition written for such an ensemble." .



###  http://www.semanticweb.org/musicOntology#OtherGenres

:OtherGenres rdf:type owl:Class ;
             
             rdfs:label "Other genres" ;
             
             rdfs:subClassOf :Genre ;
             
             rdfs:comment "Music written for the score of a play, musicals, or similar: Filmi, incidental music, video game music, music hall songs and showtunes. Regional and national music with no significant commercial impact abroad, except when it is a version of an international genre, such as: traditional music, oral traditions, sea shanties, work songs, nursery rhymes, Arabesque and indigenous music" .



###  http://www.semanticweb.org/musicOntology#OtherInstruments

:OtherInstruments rdf:type owl:Class ;
                  
                  rdfs:label "Other instruments" ;
                  
                  rdfs:subClassOf :MusicalInstrument .



###  http://www.semanticweb.org/musicOntology#Percussion

:Percussion rdf:type owl:Class ;
            
            rdfs:label "Percussion" ;
            
            rdfs:subClassOf :MusicalInstrument ;
            
            rdfs:comment "group of musical instruments, the sound of which is extracted shock or vibration (swinging) [hammers, mallets, sticks, etc. n.] on the sounding body (membrane, metal, wood, etc.)." .



###  http://www.semanticweb.org/musicOntology#Performer

:Performer rdf:type owl:Class ;
           
           rdfs:label "Performer" .



###  http://www.semanticweb.org/musicOntology#Pop

:Pop rdf:type owl:Class ;
     
     rdfs:label "Pop" ;
     
     rdfs:subClassOf :Genre ;
     
     rdfs:comment "Pop music (a term that originally derives from an abbreviation of \"popular\") is a genre of popular music, which originated in its modern form in the 1950s, deriving from rock and roll. The terms \"popular music\" and \"pop music\" are often used interchangeably, although the former is a description of music which is popular (and can include any style)." .



###  http://www.semanticweb.org/musicOntology#Prelude

:Prelude rdf:type owl:Class ;
         
         rdfs:label "Prelude" ;
         
         rdfs:subClassOf :MusicalComposition ;
         
         rdfs:comment "A prelude is a short piece of music, the form of which may vary from piece to piece. The prelude may be thought of as a preface. While, during the Baroque era, for example, it may have served as an introduction to succeeding movements of a work that were usually longer and more complex, it may also have been a standalone piece of work during the Romantic era. It generally features a small number of rhythmic and melodic motifs that recur through the piece. Stylistically, the prelude is improvisatory in nature. The prelude also may refer to an overture, particularly to those seen in an opera or an oratorio." .



###  http://www.semanticweb.org/musicOntology#Quartet

:Quartet rdf:type owl:Class ;
         
         rdfs:label "Quartet" ;
         
         rdfs:subClassOf [ rdf:type owl:Class ;
                           owl:intersectionOf ( :Collective
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :has_member ;
                                                  owl:onClass :CollectiveMember ;
                                                  owl:qualifiedCardinality "4"^^xsd:nonNegativeInteger
                                                ]
                                              )
                         ] ;
         
         rdfs:comment "In music, a quartet or quartette (French: quatuor, German: Quartett, Italian: quartetto, Spanish: cuarteto, Polish: kwartet) is an ensemble of four singers or instrumental performers; or a musical composition for four voices or instruments." .



###  http://www.semanticweb.org/musicOntology#Quintet

:Quintet rdf:type owl:Class ;
         
         rdfs:label "Quintet" ;
         
         rdfs:subClassOf [ rdf:type owl:Class ;
                           owl:intersectionOf ( :Collective
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :has_member ;
                                                  owl:onClass :CollectiveMember ;
                                                  owl:qualifiedCardinality "5"^^xsd:nonNegativeInteger
                                                ]
                                              )
                         ] ;
         
         rdfs:comment "A quintet is a group containing five members. It is commonly associated with musical groups, such as a string quintet, or a group of five singers, but can be applied to any situation where five similar or related objects are considered a single unit. In classical instrumental music, any additional instrument (such as a piano, clarinet, oboe, etc.) joined to the usual string quartet (two violins, a viola, and a cello), gives the resulting ensemble its name, such as \"piano quintet\", \"clarinet quintet\", etc. A piece of music written for such a group is similarly named." .



###  http://www.semanticweb.org/musicOntology#R&B-and-soul

<http://www.semanticweb.org/musicOntology#R&B-and-soul> rdf:type owl:Class ;
                                                        
                                                        rdfs:label "R&B and soul" ;
                                                        
                                                        rdfs:subClassOf :Genre ;
                                                        
                                                        rdfs:comment "Rhythm and blues, often abbreviated as R&B or RnB, is a genre of popular African-American music that originated in the 1940s. The term was originally used by record companies to describe recordings marketed predominantly to urban African Americans, at a time when \"urbane, rocking, jazz based music with a heavy, insistent beat\" was becoming more popular. In the commercial rhythm and blues music typical of the 1950s through the 1970s, the bands usually consisted of piano, one or two guitars, bass, drums, saxophone, and sometimes background vocalists. R&B lyrical themes often encapsulate the African-American experience of pain and the quest for freedom and joy. Lyrics focus heavily on the themes of triumphs and failures in terms of relationships, freedom, economics, aspirations, and sex." .



